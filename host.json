{
	"version": "2.0",
	"functionTimeout": "00:10:00",
	"logging": {
	  "applicationInsights": {
		"samplingSettings": {
		  "isEnabled": true,
		  "excludedTypes": "Request"
		}
	  },
	  "logLevel": {
		"Default": "Information"
	  }
	},
	"extensionBundle": {
	  "id": "Microsoft.Azure.Functions.ExtensionBundle",
	  "version": "[4.*, 5.0.0)"
	},
	"extensions": {
	  "http": {
		"routePrefix": "",
		"maxOutstandingRequests": 200,
		"maxConcurrentRequests": 100
	  }
	}
  }
  